<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inoles.nolesfootball">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:supportsRtl="false">
        <activity
            android:name=".HeadlinesActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BrowserDetailActivity"
            android:parentActivityName=".HeadlinesActivity" />
        <activity android:name=".ScheduleActivity" android:label="@string/navdrawer_schedules" />
        <activity android:name=".RostersActivity" android:label="@string/navdrawer_rosters" />
        <activity android:name=".AboutActivity" android:label="@string/about_title"/>
        <!-- <activity android:name=".StandingActivity" android:label="@string/navdrawer_standings" /> -->
    </application>

</manifest>